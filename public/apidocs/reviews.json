{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "http://localhost:3000",
  "resourcePath": "reviews",
  "apis": [
    {
      "path": "/books/{book_id}/reviews",
      "operations": [
        {
          "summary": "Create a new review for a book",
          "notes": "Create a new review for the specified book",
          "parameters": [
            {
              "paramType": "path",
              "name": "book_id",
              "type": "integer",
              "description": "ID of the book to review",
              "required": true
            },
            {
              "paramType": "form",
              "name": "review[rating]",
              "type": "integer",
              "description": "Rating for the book (1-5)",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 401,
              "responseModel": null,
              "message": "Unauthorized"
            },
            {
              "code": 406,
              "responseModel": null,
              "message": "Not Acceptable"
            },
            {
              "code": 422,
              "responseModel": null,
              "message": "Unprocessable Entity"
            }
          ],
          "nickname": "Reviews#create",
          "method": "post"
        }
      ]
    },
    {
      "path": "/books/{book_id}/reviews/{id}",
      "operations": [
        {
          "summary": "Delete a review",
          "notes": "Delete the review with the specified ID",
          "parameters": [
            {
              "paramType": "path",
              "name": "id",
              "type": "integer",
              "description": "ID of the review to be deleted",
              "required": true
            }
          ],
          "responseMessages": [
            {
              "code": 401,
              "responseModel": null,
              "message": "Unauthorized"
            },
            {
              "code": 406,
              "responseModel": null,
              "message": "Not Acceptable"
            },
            {
              "code": 422,
              "responseModel": null,
              "message": "Unprocessable Entity"
            }
          ],
          "nickname": "Reviews#destroy",
          "method": "delete"
        }
      ]
    }
  ],
  "authorizations": null
}